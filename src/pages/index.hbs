                                        <button class="configure__chosen-equipment-delete">
                                            <svg>
                                                <use xlink:href="#plus-icon"></use>
                                            </svg>
                                        </button>
                                    </div>
                                </li>
                                <li style="display: none" class="configure__chosen-equipment-wrapper">
                                    <div class="configure__chosen-equipment-more-wrapper">
                                        <button class="configure__chosen-equipment-more">
                                            еще 
                                            <span class="configure__chosen-equipment-counter"> 0</span>
                                        </button>
                                    </div>
                                </li>
                            </ul>
                            <ul class="configure__chosen-equipmentsAll">

                            </ul>
                        </fieldset>
                        <fieldset data-choise="equipment" class="configure__form-group">
                            <div class="configure__form-group-header">
                                <h3 class="configure__form-group-title">Выбрать оборудование</h3>
                                <button class="configure__form-group-button">
                                    <svg>
                                        <use xlink:href="#plus-icon"></use>
                                    </svg>
                                </button>
                            </div>
                            <div class="configure__form-group-body">
                                <ul class="configure__form-group-list">
                                    {{#each equipment }}
                                    <li class="configure__form-group-item">
                                        <input id="equipmentUaz{{@key}}"  value="{{this.title}}" data-importantCheck="true" data-configName="{{this.title}}" data-config="uaz" data-price="{{this.price}}" class="configure__form-checkbox" type="checkbox">
                                        <div class="configure__form-checkboxCustom">
                                            <svg>
                                                <use xlink:href="#checked"></use>
                                            </svg>
                                        </div>
                                        <label for="equipmentUaz{{@key}}">
                                            <div class="configure__form-text">
                                                <p>{{this.title}}</p>
                                            </div>
                                        </label>
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </fieldset>
                        <fieldset data-choise="equipment" class="configure__form-group">
