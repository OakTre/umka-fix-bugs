                                <h3 class="configure__form-group-title">Выбрать дополнительные опции</h3>
                                <button class="configure__form-group-button">
                                    <svg>
                                        <use xlink:href="#plus-icon"></use>
                                    </svg>
                                </button>
                            </div>
                            <div class="configure__form-group-body additionalOptions">
                                <ul class="configure__form-group-list">
                                    {{#each additionalOptions }}
                                    <li class="configure__form-group-item">
                                        <input id="additionalOptionsUaz{{@key}}"  value="{{this.title}}" data-configName="{{this.title}}" data-config="uaz" data-price="{{this.price}}" class="configure__form-checkbox" type="checkbox">
                                        <div class="configure__form-checkboxCustom">
